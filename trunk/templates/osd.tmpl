<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
   <ShortName>{{ name }}</ShortName>
   <Description>Use Google Custom Search to search {{ name }} and friend's websites.</Description>
   <Url type="text/html"
        template="http://www.google.com/cse?cref=http://wego-wego.appspot.com/friendfeed/{{ nickname }}/cref/&amp;q={searchTerms}"/>
   <LongName>{{ name }}'s friend's custom search</LongName>
   <Language>en-us</Language>
   <Image width="16" height="16" type="image/vnd.microsoft.icon">/favicon.ico</Image>
   <OutputEncoding>UTF-8</OutputEncoding>
   <InputEncoding>UTF-8</InputEncoding>
</OpenSearchDescription>
